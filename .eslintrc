{
    "parser": "vue-eslint-parser",
    "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "vue/no-v-for-template-key": "off"
    }
}